{
  "name": "vynxlabs",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "fetch-theme-variables": "node ./utils/fetch-theme-variables.js",
    "start": "concurrently --kill-others  \"npx tailwindcss -i ./src/css/styles.css -o ./dist/css/styles.css --watch\" \"bookshop-sass -b _component-library -o dist/css/bookshop.css -w\" \"npx eleventy --serve\"  \"sass --no-source-map --watch src/assets/styles:dist/css\" ",
    "sass:build": "sass --no-source-map src/assets/styles:dist/css",
    "bookshop-sass:build": "bookshop-sass -b _component-library -o dist/css/bookshop.css",
    "browser": "npx @bookshop/browser",
    "eleventy": "node ./utils/fetch-theme-variables.js && npx eleventy",
    "tailwind": "npx tailwindcss -i ./src/css/styles.css -o ./dist/css/styles.css --watch",
    "unlighthouse": "unlighthouse-ci --site sopriscuisine.com --build-static --root ./dist --output-path ./sopriscuisine",
    "new": "npx @bookshop/init --component"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@bookshop/browser": "^3.9.0",
    "@bookshop/eleventy-bookshop": "3.9.0",
    "@bookshop/eleventy-engine": "^3.9.0",
    "@bookshop/generate": "^3.9.0",
    "@netlify/functions": "^2.1.0",
    "@unlighthouse/cli": "^0.10.0",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-plugin-toc": "^1.1.5",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.10.2",
    "markdown-it-anchor": "^8.6.7",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.5.0",
    "sass": "^1.75.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@bookshop/sass": "^3.9.0",
    "concurrently": "^8.2.1",
    "netlify-plugin-chromium": "^1.1.4",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3"
  }
}
